        <obo:IAO_0000117 rdf:datatype="&xsd;string"
            >PERSON: Jessica Turner#funding:NIH1R01MH084812
PERSON: Angela Laird#funding:NIHR01MH084812</obo:IAO_0000117>
    </owl:Class>
    


    <!-- http://www.cogpo.org/ontologies/CogPOver1.owl#COGPO_00110 -->

    <owl:Class rdf:about="&ontologies;CogPOver1.owl#COGPO_00110">
        <rdfs:label
            >Oddball Target Tones Condition</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;OBI_0000011"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
