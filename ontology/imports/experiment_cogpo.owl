PERSON: Angela Laird#funding:NIHR01MH084812</obo:IAO_0000117>
    </owl:Class>
    


    <!-- http://www.cogpo.org/ontologies/CogPOver1.owl#COGPO_00122 -->

    <owl:Class rdf:about="&ontologies;CogPOver1.owl#COGPO_00122">
        <rdfs:label>Stimulus</rdfs:label>
        <rdfs:subClassOf rdf:resource="&snap;ObjectAggregate"/>
        <dc:date rdf:datatype="&xsd;string">April 13 2010</dc:date>
